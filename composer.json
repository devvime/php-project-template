{
    "name": "devvime/php-project-template",
    "autoload": {
        "psr-4": {
            "App\\": "App/"
        }
    },
    "scripts": {
        "migration:create": "vendor/bin/phinx create",
        "migration:run": "vendor/bin/phinx migrate",
        "seed:create": "vendor/bin/phinx seed:create",
        "seed:run": "vendor/bin/phinx seed:run",
        "rollback": "vendor/bin/phinx rollback",
        "rollback:step": "vendor/bin/phinx rollback -t 0"
    },
    "license": "MIT",
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "robmorgan/phinx": "^0.16.10",
        "devvime/modpath": "^1.0"
    }
}
